function calculator(a, b, operation) {
  // Check if a and b are numbers
  if (typeof a !== 'number' || typeof b !== 'number') {
    return "unknown value";
  }

  // Perform the operation
  switch (operation) {
    case '+':
      return a + b;
    case '-':
      return a - b;
    case '*':
      return a * b;
    case '/':
      return b !== 0 ? a / b : "unknown value";
    default:
      return "unknown value";
  }
}